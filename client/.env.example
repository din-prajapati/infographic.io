# Frontend Environment Variables (Public)
# Copy this file to .env.development or .env.production and fill in your values

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_API_BASE=/api/v1
VITE_CLIENT_URL=http://localhost:5173

# Payment Provider Public Keys (Publishable keys only - safe to expose)
VITE_RAZORPAY_KEY_ID=rzp_test_YOUR_KEY_ID
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_YOUR_KEY

# Feature Flags
VITE_ENABLE_STRIPE=false
VITE_ENABLE_ANALYTICS=true
